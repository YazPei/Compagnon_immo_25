{
	"dashboard": {
		"id": null,
		"title": "Métriques des Modèles et API",
		"tags": [
			"ml",
			"api",
			"monitoring"
		],
		"timezone": "browser",
		"panels": [
			{
				"id": 1,
				"title": "Requêtes HTTP Totales",
				"type": "graph",
				"targets": [
					{
						"expr": "rate(http_requests_total[5m])",
						"legendFormat": "{{method}} {{endpoint}} {{status_code}}"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 0,
					"y": 0
				}
			},
			{
				"id": 2,
				"title": "Durée des Requêtes HTTP",
				"type": "graph",
				"targets": [
					{
						"expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
						"legendFormat": "95th percentile"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 12,
					"y": 0
				}
			},
			{
				"id": 3,
				"title": "Prédictions de Modèles",
				"type": "graph",
				"targets": [
					{
						"expr": "rate(model_predictions_total[5m])",
						"legendFormat": "{{model_name}} v{{model_version}}"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 0,
					"y": 8
				}
			},
			{
				"id": 4,
				"title": "Durée des Prédictions",
				"type": "graph",
				"targets": [
					{
						"expr": "histogram_quantile(0.95, rate(model_prediction_duration_seconds_bucket[5m]))",
						"legendFormat": "{{model_name}} v{{model_version}}"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 12,
					"y": 8
				}
			},
			{
				"id": 5,
				"title": "Erreurs de Modèles",
				"type": "graph",
				"targets": [
					{
						"expr": "rate(model_errors_total[5m])",
						"legendFormat": "{{model_name}} {{error_type}}"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 0,
					"y": 16
				}
			},
			{
				"id": 6,
				"title": "Demandes d'Estimation",
				"type": "graph",
				"targets": [
					{
						"expr": "rate(estimation_requests_total[5m])",
						"legendFormat": "{{property_type}} {{region}}"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 12,
					"y": 16
				}
			}
		],
		"time": {
			"from": "now-1h",
			"to": "now"
		},
		"timepicker": {},
		"templating": {
			"list": []
		},
		"annotations": {
			"list": []
		},
		"refresh": "5s",
		"schemaVersion": 27,
		"version": 0,
		"links": []
	}
}